{
  "name": "ankichess",
  "private": true,
  "version": "1.2.00",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:anki-media": "vite build --config vite.config.anki.ts",
    "build:anki": "npm run build:anki-media && uv run scripts/build_apkg.py",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json && npm run check:deps",
    "check:error": "svelte-check --threshold error --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "check:deps": "depcruise --ignore-known .dependency-cruiser-known-violations.json --output-type err src"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.2.53",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/archiver": "^7.0.0",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "archiver": "^7.0.1",
    "dependency-cruiser": "^17.2.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^3.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.93.3",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "svelte-render-scan": "^1.1.0",
    "typescript": "~5.9.3",
    "typescript-svelte-plugin": "^0.3.50",
    "unplugin-icons": "^23.0.1",
    "vite": "^7.1.7",
    "vite-plugin-eslint2": "^5.0.4"
  },
  "dependencies": {
    "@lichess-org/chessground": "^9.8.5",
    "@mliebelt/pgn-parser": "^1.4.19",
    "chess.js": "^1.4.0",
    "material-symbols": "^0.40.2",
    "stockfish": "^17.1.0"
  }
}
